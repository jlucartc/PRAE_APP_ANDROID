<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luca.prae_app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher_prae_app_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_prae_app_launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NoticiasActivity"
            android:label="@string/title_activity_noticias"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NoticiaWebViewActivity"
            android:label="@string/title_activity_web_view" />
        <activity
            android:name=".AjudaDeCustoActivity"
            android:label="@string/title_activity_ajuda_de_custo" />
        <activity
            android:name=".AcolhimentoAoEstudanteActivity"
            android:label="@string/title_activity_acolhimento_ao_estudante" />
        <activity
            android:name=".BolsasAuxiliosActivity"
            android:label="@string/title_activity_bolsas_auxilio" />
        <activity
            android:name=".RestauranteUniversitarioActivity"
            android:label="@string/title_activity_restaurante_universitario" />
        <activity
            android:name=".CalendarioDeAtividadesActivity"
            android:label="@string/title_activity_calendario_de_atividades" />
        <activity
            android:name=".MapaDaPraeActivity"
            android:label="@string/title_activity_mapa_da_prae" />
        <activity
            android:name=".VoceSabiaActivity"
            android:label="@string/title_activity_voce_sabia" />
        <activity
            android:name=".FaleConoscoActivity"
            android:label="@string/title_activity_fale_conosco" />
        <activity
            android:name=".AjudaDeCustoDocumentacaoNecessariaActivity"
            android:label="@string/title_activity_ajuda_de_custo_documentacao_necessaria"></activity>
    </application>

</manifest>